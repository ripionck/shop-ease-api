{
  "version": 2,
  "builds": [
    {
      "src": "shop_ease_api/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.10"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "shop_ease_api/wsgi.py"
    }
  ],
  "functions": {
    "app/api/**/*": {
      "maxDuration": 25
    }
  }
}
